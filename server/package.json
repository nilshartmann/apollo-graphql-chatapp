{
  "name": "vscode-typescript-starter",
  "version": "1.0.0",
  "main": "dist/main.js",
  "license": "MIT",
  "author": {
    "email": "nils@nilshartmann.net",
    "name": "Nils Hartmann",
    "url": "https://nilshartmann.net"
  },
  "description": "A simple starter workspace for TypeScript Development with Visual Studio code",
  "keywords": [
    "visual studio code",
    "typescript",
    "javascript",
    "vscode",
    "starter kit"
  ],
  "scripts": {
    "build": "tsc",
    "compile:watch": "tsc --watch",
    "start": "nodemon",
    "clean": "rimraf dist/",
    "prettier": "prettier --write \"{src}/**/*.{js,jsx,ts,tsx}\"",
    "gql:schema": "apollo-codegen introspect-schema http://localhost:3000/graphql --output chatapp-schema.json",
    "gql:codegen": "apollo-codegen generate src/**/*.graphql --schema chatapp-schema.json --target typescript --output operation-result-types.ts"
  },
  "devDependencies": {
    "nodemon": "^1.17.2",
    "prettier": "^1.11.0",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "@types/graphql": "^0.12.5",
    "apollo-server-express": "^1.3.2",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tools": "^2.23.1",
    "subscriptions-transport-ws": "^0.9.6",
    "tslint": "^5.7.0"
  }
}
